
@{
    ViewBag.Title = "Index";
}

<section class="hero-wrap hero-wrap-2 js-fullheight" style="background-image: url('../../content/images/bg_3.jpg');" data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="row no-gutters slider-text js-fullheight align-items-end justify-content-start">
            <div class="col-md-9 ftco-animate pb-5">
                <p class="breadcrumbs"><span class="mr-2"><a href="~/Home">Trang chủ <i class="ion-ios-arrow-forward"></i></a></span> <span>Giá cả <i class="ion-ios-arrow-forward"></i></span></p>
                <h1 class="mb-3 bread">Giá các loại xe</h1>
            </div>
        </div>
    </div>
</section>

<section class="ftco-section ftco-cart">
    <div class="container">
        <div class="row">
            <div class="col-md-12 ftco-animate">
                <div class="car-list">
                    <table class="table">
                        <thead class="thead-primary">
                            <tr class="text-center">
                                <th>&nbsp;</th>
                                <th>&nbsp;</th>
                                <th class="bg-primary heading">Giá theo giờ</th>
                                <th class="bg-dark heading">Giá theo ngày</th>
                                <th class="bg-black heading">Giá theo tháng</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{ Html.RenderAction("Price_Type", "Pricing");}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>

