@foreach (var item in Model)
{
    <tr class="">
        <td class="car-image"><div class="img" style="background-image:url(@item.image);"></div></td>
        <td class="product-name">
            <h3>@item.type1</h3>
            <p class="mb-0 rated">
                <span>Đánh giá:</span>
                <span class="ion-ios-star"></span>
                <span class="ion-ios-star"></span>
                <span class="ion-ios-star"></span>
                <span class="ion-ios-star"></span>
                <span class="ion-ios-star"></span>
            </p>
        </td>

        <td class="price">
            <p class="btn-custom">@Html.ActionLink("Thuê xe","Type",new {type =item.type1})</p>
            <div class="price-rate">
                <h3>
                    <span class="num">@item.price_hour<small>Đ</small></span>
                    <span class="per">/mỗi giờ</span>
                </h3>
            </div>
        </td>

        <td class="price">
            <p class="btn-custom">@Html.ActionLink("Thuê xe", "Type", new { type = item.type1 })</p>
            <div class="price-rate">
                <h3>
                    <span class="num">@item.price_day<small>Đ</small></span>
                    <span class="per">/mỗi ngày</span>
                </h3>
            </div>
        </td>

        <td class="price">
            <p class="btn-custom">@Html.ActionLink("Thuê xe", "Type", new { type = item.type1 })</p>
            <div class="price-rate">
                <h3>
                    <span class="num">@item.price_month<small>Đ</small></span>
                    <span class="per">/mỗi tháng</span>
                </h3>
            </div>
        </td>
    </tr>

}
